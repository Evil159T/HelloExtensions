{
  "name": "Clicker Extension",
  "description": "Contador global de clics con mejoras y clics autom√°ticos.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [{
    "resources": ["floating.html", "styles.css", "clicker.png"],
    "matches": ["<all_urls>"]
  }]
}
